{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "src": "/admin/(.*)",
            "dest": "/admin"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}
